import random
def find_pairs(dividend):
    pairs = []
    for i in range(1, 21):
        for j in range(1, 21):
            if (i + j) != 0 and dividend % (i +j) == 0:
                pairs.append((i, j))
    return pairs
random_number = random.randint(3, 20)
print(f"Случайное число (первое поле): {random_number}")
result_pairs = find_pairs(random_number)
if result_pairs:
    print("Найденные пары чисел (i, j):")
    for pair in result_pairs:
        print(pair)
else:
    print("Пары чисел не найдены.")
